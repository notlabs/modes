{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": "always"
  },
  "files.exclude": {
    "**/dist": true,
    "**/.nx/cache": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/dist": true,
    "**/.nx": true,
    "**/node_modules": true
  }
}
